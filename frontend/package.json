{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:check": "npm run lint && npm run build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "lint:fix": "eslint src --ext .ts,.tsx,.js --fix",
    "lint:all": "eslint . --ext .ts,.tsx,.js",
    "prepare": "husky",
    "test": "node --no-deprecation ./node_modules/.bin/jest",
    "restart": "sh ./scripts/restart-dev.sh",
    "diagnose": "node ./scripts/diagnostics/simple-diagnostics.js",
    "find-unused-translations": "node ./scripts/diagnostics/find-unused-translations.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@iconify/react": "^6.0.0",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.9.0",
    "cookies-next": "^5.1.0",
    "dompurify": "^3.2.6",
    "eslint-config-next": "^15.3.2",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "keen-slider": "^6.8.6",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "punycode": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.1",
    "react-icons": "^5.5.0",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
